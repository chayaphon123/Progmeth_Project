<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.home.HomeController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 50;">
         <children>
            <ImageView fitHeight="540.0" fitWidth="897.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../assets/images/logo.jpg" />
               </image>
            </ImageView>
            <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#buttonHandler" style="-fx-background-radius: 25;" text="START">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</StackPane>
